<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape
            android:shape="rectangle">


            <size
                android:width="100dp"
                android:height="20dp"/>
            <stroke
                android:width="1dp"
                android:color="#776200EE"/>
            <corners
                android:radius="4dp"/>
        </shape>
    </item>

    <item>
        <shape
            android:shape="rectangle">
            <size
                android:width="100dp"
                android:height="20dp"/>
            <stroke
                android:width="1dp"
                android:color="#6200EE"/>
            <corners
                android:radius="4dp"/>

        </shape>

    </item></selector>